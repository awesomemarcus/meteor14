{
  "name": "mantra-boilerplate",
  "version": "0.1.0",
  "description": "mantra-boilerplate",
  "scripts": {
    "start": "./meteor.sh run",
    "android-device": "./meteor.sh run android-device",
    "version": "./version.sh > imports/settings/version.js",
    "test": "istanbul cover --dir .coverage  _mocha -- .scripts/mocha_boot.js client/**/tests/**/*.js --compilers js:babel-core/register",
    "test-watch": "istanbul cover --dir .coverage  _mocha -- .scripts/mocha_boot.js client/**/tests/**/*.js --compilers js:babel-core/register --watch",
    "storybook": "start-storybook -p 9001 -s ./public",
    "gagarin": "gagarin -Bmv",
    "gagarin-build": "gagarin --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/username/reponame.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@kadira/storybook": "2.3.0",
    "babel-core": "6.13.2",
    "babel-plugin-react-require": "2.1.0",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-2": "6.13.0",
    "babel-root-slash-import": "1.1.0",
    "bootstrap": "3.3.7",
    "bootstrap-webpack": "0.0.5",
    "chai": "3.x.x",
    "css-loader": "0.23.1",
    "enzyme": "2.4.1",
    "file-loader": "0.9.0",
    "jsdom": "9.4.2",
    "less-loader": "2.2.3",
    "mocha": "3.0.2",
    "raw-loader": "0.5.1",
    "sinon": "1.17.5",
    "style-loader": "0.13.1"
  },
  "dependencies": {
    "mantra-core": "1.7.0",
    "moment": "2.14.1",
    "node-simple-schema": "0.0.4",
    "react": "15.3.0",
    "react-dom": "15.3.0",
    "react-mounter": "1.2.0",
    "underscore": "1.8.3"
  }
}
